#1
def add(b,c):
    print(b+c)
print(add(1,2) + add(2,3))
#answer is 3 and 5 as you are putting seperate numbers next to each other.

#2
def concatenate(b,c):
    return str(b)+str(c)
print(concatenate(2,5))
#answer is 25 as  you are adding individual number 2 and 5 together

#3
def num():
    b = 100
    print(b)
    if b < 10:
        return 5
    else:
        return 10
    return 7
print(num())
#answer is 100 and 10
